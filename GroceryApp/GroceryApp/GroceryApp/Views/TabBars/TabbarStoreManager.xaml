<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:d="http://xamarin.com/schemas/2014/forms/design"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d"
            
            BarBackgroundColor="#FAFAFA"
            SelectedTabColor="{StaticResource RedTheme}"
            UnselectedTabColor="#A6AAA9"
             xmlns:local="clr-namespace:GroceryApp.Views.Screens"
             x:Class="GroceryApp.Views.TabBars.TabbarStoreManager"
            
            x:Name="TabManager"
            NavigationPage.HasNavigationBar="False">
    
    <local:StoreDashBoardView Title="Dashboard" IconImageSource="dashboard"/>
    <local:ProductManagerView Title="Product" IconImageSource="box"/>
    <local:OrderManagerView Title="Orders" IconImageSource="list"/>
    <local:ReviewManagerView Title="Feedbacks" IconImageSource="feedback"/>
    <local:StoreSettingView Title="Settings" IconImageSource="blacksetting"/>
</TabbedPage>